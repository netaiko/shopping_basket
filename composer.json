{
  "name": "netaiko/shopping_basket",
  "description": "This is a shopping basket example.",
  "type": "project",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Jose M. Arias Morales"
    }
  ]
}
